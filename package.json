{
  "name": "@cxphoenix/slidev-theme-fhsh-isiphs-universal",
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/CXPhoenix/slidev-theme-fhsh-isiphs-universal.git"
  },
  "packageManager": "pnpm@10.5.2",
  "homepage": "https://github.com/CXPhoenix/slidev-theme-fhsh-isiphs-universal#readme",
  "keywords": [
    "slidev-theme",
    "slidev",
    "fhsh",
    "aisp",
    "isip.hs",
    "universal"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^52.1.0",
    "@vueuse/core": "^13.6.0",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "@slidev/cli": "^52.1.0",
    "playwright-chromium": "^1.50.1"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "light",
    "defaults": {
      "fonts": {
        "sans": "edukai, Fira Mono, Noto Sans TC",
        "serif": "Zen Antique, Noto Serif Traditional Chinese",
        "mono": "Source Code Pro, Noto Sans Mono",
        "local": "edukai"
      },
      "layout": "default",
      "aspectRadio": "16/9",
      "mdc": true,
      "titleTemplate": "%s | FHSH AiSP X ISIP.hs",
      "favicon": "https://security-program.fhsh.taipei/New AiSP LOGO.png"
    }
  }
}